{
  "name": "@notabug/gun-suppressor-sear",
  "version": "0.2.0",
  "description": "SEA Read/Validation support for gun-suppressor validation",
  "main": "lib/@notabug/gun-suppressor-sear.js",
  "scripts": {
    "build": "webpack --env dev && webpack --env build && npm run test",
    "dev": "webpack --progress --colors --watch --env dev",
    "test": "mocha --require babel-register --colors ./test/*.spec.js",
    "test:watch": "mocha --require babel-register --colors -w ./test/*.spec.js",
    "test:cover": "cross-env NODE_ENV=test nyc mocha --require babel-register --colors test/*.js",
    "repl": "node -i -e \"$(< ./lib/@notabug/gun-suppressor-sear.js)\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/notabugio/gun-suppressor-sear.git"
  },
  "keywords": [
    "gundb",
    "json-schema"
  ],
  "author": "go1dfish",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/notabugio/gun-suppressor-sear/issues"
  },
  "homepage": "https://github.com/notabugio/gun-suppressor-sear",
  "dependencies": {
    "@notabug/gun-suppressor": "^0.1.0",
    "ajv": "^6.6.2",
    "ramda": "^0.26.1",
    "route-parser": "^0.0.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.51",
    "@babel/core": "^7.0.0-beta.51",
    "@babel/preset-env": "^7.0.0-beta.51",
    "ajv": "^6.6.2",
    "babel-eslint": "^8.0.3",
    "babel-loader": "^8.0.0-beta.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-istanbul": "^5.1.0",
    "babel-preset-env": "^7.0.0-beta.3",
    "babel-register": "^7.0.0-beta.3",
    "chai": "^4.1.2",
    "cross-env": "^5.2.0",
    "eslint": "^5.0.1",
    "eslint-loader": "^2.0.0",
    "jsdom": "11.11.0",
    "jsdom-global": "3.0.2",
    "mocha": "^4.0.1",
    "nyc": "^13.1.0",
    "prettier": "1.15.3",
    "ramda": "^0.26.1",
    "route-parser": "^0.0.5",
    "uglifyjs-webpack-plugin": "^1.2.7",
    "webpack": "^4.12.2",
    "webpack-cli": "^3.0.8",
    "yargs": "^10.0.3"
  },
  "nyc": {
    "sourceMap": false,
    "instrument": false
